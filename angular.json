{
  "version": 1,
  "projects": {
    "eam-js": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/eam-js",
      "sourceRoot": "apps/eam-js/src",
      "prefix": "eam-js",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/eam-js",
            "index": "apps/eam-js/src/index.html",
            "main": "apps/eam-js/src/main.ts",
            "polyfills": "apps/eam-js/src/polyfills.ts",
            "tsConfig": "apps/eam-js/tsconfig.app.json",
            "aot": false,
            "assets": ["apps/eam-js/src/favicon.ico", "apps/eam-js/src/assets"],
            "styles": [
              "node_modules/material-design-icons-iconfont/dist/material-design-icons.css",
              "apps/eam-js/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/eam-js/src/environments/environment.ts",
                  "with": "apps/eam-js/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "eam-js:build",
            "proxyConfig": "apps/eam-js/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "eam-js:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "eam-js:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/eam-js/tsconfig.app.json",
              "apps/eam-js/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/eam-js/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/eam-js/jest.config.js",
            "tsConfig": "apps/eam-js/tsconfig.spec.json",
            "setupFile": "apps/eam-js/src/test-setup.ts"
          }
        }
      }
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "custom": {
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.custom.ts"
                }
              ]
            },
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          },
          "configurations": {
            "custom": {
              "buildTarget": "api:build:custom"
            },
            "production": {
              "buildTarget": "api:build:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/api/tsconfig.app.json",
              "apps/api/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/api/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "tsConfig": "apps/api/tsconfig.spec.json"
          }
        }
      }
    },
    "common-navigation": {
      "projectType": "library",
      "root": "libs/common/navigation",
      "sourceRoot": "libs/common/navigation/src",
      "prefix": "eam-js",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/common/navigation/tsconfig.lib.json",
              "libs/common/navigation/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/common/navigation/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/common/navigation/jest.config.js",
            "tsConfig": "libs/common/navigation/tsconfig.spec.json",
            "setupFile": "libs/common/navigation/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    },
    "projects-overview-shell": {
      "projectType": "library",
      "root": "libs/projects/overview-shell",
      "sourceRoot": "libs/projects/overview-shell/src",
      "prefix": "eam-js",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/projects/overview-shell/tsconfig.lib.json",
              "libs/projects/overview-shell/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/projects/overview-shell/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/projects/overview-shell/jest.config.js",
            "tsConfig": "libs/projects/overview-shell/tsconfig.spec.json",
            "setupFile": "libs/projects/overview-shell/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    },
    "projects-api": {
      "root": "libs/projects/api",
      "sourceRoot": "libs/projects/api/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/projects/api/tsconfig.lib.json",
              "libs/projects/api/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/projects/api/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/projects/api/jest.config.js",
            "tsConfig": "libs/projects/api/tsconfig.spec.json"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "eam-js"
}
