{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "eam-js",
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest",
      "style": "scss"
    },
    "@nrwl/angular:component": {
      "styleext": "scss"
    },
    "@schematics/angular": {
      "component": {
        "changeDetection": "OnPush"
      }
    }
  },
  "projects": {
    "eam-js": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/eam-js",
      "sourceRoot": "apps/eam-js/src",
      "prefix": "",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/eam-js",
            "index": "apps/eam-js/src/index.html",
            "main": "apps/eam-js/src/main.ts",
            "polyfills": "apps/eam-js/src/polyfills.ts",
            "tsConfig": "apps/eam-js/tsconfig.app.json",
            "assets": ["apps/eam-js/src/favicon.ico", "apps/eam-js/src/assets"],
            "styles": [
              "node_modules/material-design-icons-iconfont/dist/material-design-icons.css",
              "apps/eam-js/src/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["libs/"]
            },
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/eam-js/src/environments/environment.ts",
                  "with": "apps/eam-js/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "outputs": ["{options.outputPath}"]
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "eam-js:build",
            "proxyConfig": "apps/eam-js/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "eam-js:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "eam-js:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/eam-js/src/**/*.ts",
              "apps/eam-js/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/eam-js/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/eam-js"]
        }
      }
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"],
            "buildLibsFromSource": true
          },
          "configurations": {
            "custom": {
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.custom.ts"
                }
              ]
            },
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          },
          "outputs": ["{options.outputPath}"]
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          },
          "configurations": {
            "custom": {
              "buildTarget": "api:build:custom"
            },
            "production": {
              "buildTarget": "api:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/api/src/**/*.ts",
              "apps/api/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/api"]
        }
      }
    },
