    "core-js": "^2.6.11",
    "helmet": "^3.21.2",
    "jwt-decode": "^2.2.0",
    "material-design-icons-iconfont": "^5.0.1",
    "mysql": "^2.18.1",
    "ngx-quill": "^7.3.12",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "quill": "^1.3.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.5.4",
    "swagger-ui-express": "^4.1.3",
    "tslib": "^1.10.0",
    "typeorm": "^0.2.22",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular/cli": "9.0.1",
    "@angular/compiler-cli": "^9.0.0",
    "@angular/language-service": "^9.0.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@nestjs/schematics": "^6.9.3",
    "@nestjs/testing": "^6.11.5",
    "@ngneat/spectator": "^4.11.1",
    "@ngrx/schematics": "8.6.0",
    "@ngrx/store-devtools": "8.6.0",
    "@nrwl/jest": "9.0.0",
    "@nrwl/nest": "9.0.0",
    "@nrwl/node": "9.0.0",
    "@nrwl/workspace": "9.0.0",
    "@types/jest": "24.0.9",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "conventional-changelog-cli": "^2.0.31",
    "dotenv": "6.2.0",
    "eslint": "6.1.0",
    "husky": "^4.2.1",
    "jest": "24.1.0",
    "jest-preset-angular": "8.0.0",
    "lint-staged": "^10.0.7",
    "ngrx-tslint-rules": "^1.16.0",
    "prettier": "1.19.1",
    "rxjs-tslint-rules": "^4.28.1",
    "ts-jest": "24.0.0",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~3.7.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,md,css,scss,html}": [
      "prettier --write"
    ],
    "yarn.lock": [
      "git rm --cached"
    ]
  }
}
