    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@nestjs/schematics": "^6.9.4",
    "@nestjs/testing": "^6.11.11",
    "@ngneat/spectator": "^5.6.1",
    "@ngrx/schematics": "9.1.0",
    "@ngrx/store-devtools": "9.1.0",
    "@nrwl/jest": "9.3.0",
    "@nrwl/nest": "9.3.0",
    "@nrwl/node": "9.3.0",
    "@nrwl/workspace": "9.3.0",
    "@types/jest": "25.1.4",
    "@types/node": "^12.12.37",
    "codelyzer": "^5.2.2",
    "conventional-changelog-cli": "^2.0.34",
    "dotenv": "6.2.0",
    "eslint": "6.8.0",
    "husky": "^4.2.5",
    "jest": "25.2.4",
    "jest-preset-angular": "8.1.3",
    "lint-staged": "^10.1.7",
    "ngrx-tslint-rules": "^2.0.0",
    "prettier": "^2.0.5",
    "rxjs-tslint-rules": "^4.31.0",
    "ts-jest": "25.2.1",
    "ts-node": "~7.0.1",
    "tslint": "6.0.0",
    "typescript": "~3.8.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,md,css,scss,html}": [
      "prettier --write"
    ],
    "yarn.lock": [
      "git rm --cached"
    ]
  }
}
