    "core-js": "^2.5.4",
    "material-design-icons-iconfont": "^5.0.1",
    "mysql": "^2.17.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "~6.4.0",
    "typeorm": "^0.2.19",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.3",
    "@angular/cli": "8.3.3",
    "@angular/compiler-cli": "^8.2.0",
    "@angular/language-service": "^8.2.0",
    "@commitlint/cli": "^8.2.0",
