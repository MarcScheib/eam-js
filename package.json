    "material-design-icons-iconfont": "^5.0.1",
    "mysql": "^2.18.1",
    "ngx-quill": "^8.1.7",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "quill": "^1.3.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5",
    "swagger-ui-express": "^4.1.4",
    "tslib": "^1.11.1",
    "typeorm": "^0.2.24",
    "zone.js": "~0.10.3"
